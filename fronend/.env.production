# Production Environment Configuration for Axie Studio
# This file ensures sensitive information is properly handled in production builds

# Disable source maps in production to prevent code inspection
GENERATE_SOURCEMAP=false

# Disable console logs in production build
REACT_APP_DISABLE_CONSOLE_LOGS=true

# Security settings
REACT_APP_ENVIRONMENT=production
REACT_APP_SECURITY_MODE=strict

# Build optimization
REACT_APP_BUILD_MODE=production
INLINE_RUNTIME_CHUNK=false

# Disable development features
REACT_APP_DEV_TOOLS=false
REACT_APP_DEBUG_MODE=false

# API Configuration (obfuscated)
REACT_APP_API_ENDPOINT_OBFUSCATED=true
